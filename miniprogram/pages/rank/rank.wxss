/* pages/rank/rank.wxss */
page{
  background: #783c9e;
  height: 100%;
}
.listBox{
  background: white;
  border-radius: 20rpx;
  width: 700rpx;
  margin-left: auto;
  margin-right: auto;
}
.tip{
  text-align: center;
  font-size: 28rpx;
  color: #f45397;
  padding: 20rpx 0;
}
.tip2{
  text-align: center;
  font-size: 24rpx;
  color: white;
  padding: 20rpx 0;
}
.item{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 10rpx 20rpx;
  align-items: center;
  border-bottom: solid 2rpx #ebebeb;
}
.left{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.right{
  font-size: 48rpx;
  color: #f75297;
}
.head{
  width: 60rpx;
  height: 60rpx;
  border-radius: 30rpx;
  margin: 10rpx 20rpx;
}
.my{
  text-align: center;
  font-size: 30rpx;
  color: #783c9e;
  margin-top: 56rpx;
}